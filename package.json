{
  "name": "apollo-boost",
  "version": "0.4.4",
  "description": "The easiest way to get started with Apollo Client",
  "author": "Peggy Rayzis <peggy@apollographql.com>",
  "contributors": [
    "James Baxley <james@apollographql.com>",
    "Sashko Stubailo <sashko@apollographql.com>",
    "James Burgess <jamesmillerburgess@gmail.com>"
  ],
  "license": "MIT",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/apollo-client.git"
  },
  "bugs": {
    "url": "https://github.com/apollographql/apollo-client/issues"
  },
  "homepage": "https://github.com/apollographql/apollo-client#readme",
  "scripts": {
    "prepare": "npm run build",
    "test": "tsc -p tsconfig.json --noEmit && jest",
    "coverage": "jest --coverage",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts",
    "prebuild": "npm run clean",
    "build": "tsc -b .",
    "postbuild": "npm run bundle",
    "watch": "tsc -w -p .",
    "clean": "rm -rf coverage/* lib/*",
    "prepublishOnly": "npm run build",
    "bundle": "rollup -c rollup.config.js"
  },
  "dependencies": {
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "git+https://github.com/Gazler/apollo-client.git#gr-apollo-client",
    "apollo-link": "^1.0.6",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.1",
    "graphql-tag": "^2.4.2",
    "ts-invariant": "^0.4.0",
    "tslib": "1.10.0"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-modules-umd": "7.2.0",
    "@condenast/bundlesize": "0.18.1",
    "@octokit/rest": "16.30.1",
    "@types/benchmark": "1.0.31",
    "@types/graphql": "14.2.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.139",
    "@types/node": "12.7.5",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "benchmark": "2.1.4",
    "check-if-folder-contents-changed-in-git-commit-range": "1.0.0",
    "codecov": "3.5.0",
    "danger": "7.1.4",
    "fetch-mock": "7.3.9",
    "flow-bin": "0.107.0",
    "graphql": "14.5.6",
    "graphql-tag": "2.10.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "lerna": "3.16.4",
    "lint-staged": "8.2.1",
    "lodash": "4.17.15",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rollup": "1.17.0",
    "rollup-plugin-invariant": "0.5.6",
    "rollup-plugin-local-resolve": "1.0.7",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript2": "0.18.1",
    "rxjs": "6.5.3",
    "ts-jest": "23.10.5",
    "tsc-watch": "2.4.0",
    "tslib": "1.10.0",
    "tslint": "5.20.0",
    "typescript": "3.5.3",
    "uglify-js": "3.6.0",
