{
  "name": "apollo-client",
  "version": "2.6.4",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "coverage": "jest --coverage",
    "dev": "./scripts/dev.sh",
    "deploy": "./scripts/deploy.sh",
    "test": "tsc -p tsconfig.test.json --noEmit && jest",
    "benchmark": "npm run build:benchmark && node benchmark_lib/benchmark/index.js",
    "benchmark:inspect": "npm run build:benchmark && node --inspect --debug-brk benchmark_lib/benchmark/index.js",
    "type-check": "flow check",
    "prebuild": "npm run clean",
    "build": "tsc -b .",
    "postbuild": "npm run bundle",
    "build:benchmark": "tsc -p tsconfig.benchmark.json",
    "watch": "../../node_modules/tsc-watch/index.js --onSuccess \"npm run postbuild\"",
    "bundle": "rollup -c rollup.config.js",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts",
    "testonly": "jest",
    "preanalyze": "npm run build",
    "analyze": "webpack -p --config analyze/webpack.config.js",
    "clean": "rm -rf coverage/* lib/*"
  },
  "repository": {
    "type": "git",
    "url": "apollographql/apollo-client"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "@types/zen-observable": "^0.8.0",
    "apollo-cache": "^1.3.2",
    "apollo-link": "^1.0.0",
    "apollo-utilities": "git+https://github.com/Gazler/apollo-client.git#gr-apollo-utilities",
    "symbol-observable": "^1.0.2",
    "ts-invariant": "^0.4.0",
    "tslib": "1.10.0",
    "zen-observable": "^0.8.0"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-modules-umd": "7.2.0",
    "@condenast/bundlesize": "0.18.1",
    "@octokit/rest": "16.30.1",
    "@types/benchmark": "1.0.31",
    "@types/graphql": "14.2.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.139",
    "@types/node": "12.7.5",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "benchmark": "2.1.4",
    "check-if-folder-contents-changed-in-git-commit-range": "1.0.0",
    "codecov": "3.5.0",
    "danger": "7.1.4",
    "fetch-mock": "7.3.9",
    "flow-bin": "0.107.0",
    "graphql": "14.5.6",
    "graphql-tag": "2.10.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "lerna": "3.16.4",
    "lint-staged": "8.2.1",
    "lodash": "4.17.15",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rollup": "1.17.0",
    "rollup-plugin-invariant": "0.5.6",
    "rollup-plugin-local-resolve": "1.0.7",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript2": "0.18.1",
    "rxjs": "6.5.3",
    "ts-jest": "23.10.5",
    "tsc-watch": "2.4.0",
    "tslib": "1.10.0",
    "tslint": "5.20.0",
    "typescript": "3.5.3",
    "uglify-js": "3.6.0",
    "webpack": "4.40.2",
    "webpack-bundle-analyzer": "3.5.0"
  }
}
